{
    "name": "@kaiserleap/dummies-factory",
    "version": "1.0.1",
    "description": "Create dummy object arrays using a one-liner!",
    "author": "Kaiser Leap - Mustafa Alhasanat",
    "license": "ISC",
    "main": "./dist/index.js",
    "type": "module",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        }
    },
    "files": [
        "dist",
        "dist/lib/",
        "lib/**/*.(ts|js)"
    ],
    "scripts": {
        "test": "jest",
        "build": "rimraf ./dist && tsc",
        "format": "prettier --write .",
        "lint": "eslint ."
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/MustafaHasanat/dummies-factory.git"
    },
    "keywords": [
        "javascript",
        "blocks",
        "npm-tool"
    ],
    "bugs": {
        "url": "https://github.com/MustafaHasanat/dummies-factory/issues"
    },
    "homepage": "https://github.com/MustafaHasanat/dummies-factory#readme",
    "release": {
        "branches": [
            "main"
        ]
    },
    "dependencies": {
        "express": "^4.18.2",
        "rimraf": "^5.0.5"
    },
    "devDependencies": {
        "@types/jest": "^29.5.7",
        "@types/node": "^20.8.10",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "babel-cli": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "eslint": "^8.56.0",
        "jest": "^29.7.0",
        "prettier": "^3.1.1",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2"
    }
}
